{
  "name": "@listwell/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch --env-file=.env.local src/index.ts",
    "build": "tsup",
    "start": "node dist/migrate.js && node dist/index.js",
