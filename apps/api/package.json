    "@google/genai": "^1.41.0",
    "@hono/node-server": "^1.14.3",
    "@listwell/db": "workspace:*",
    "@listwell/shared": "workspace:*",
    "@parse/node-apn": "^7.1.0",
    "@vercel/blob": "^2.2.0",
    "better-auth": "^1.4.18",
    "drizzle-orm": "^0.45.1",
    "e2b": "^1.2.3",
    "hono": "^4.7.14",
    "inngest": "^3.52.0",
    "openai": "^6.22.0",
    "postgres": "^3.4.8",
    "web-push": "^3.6.7",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/web-push": "^3.6.4",
